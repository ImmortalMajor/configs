{
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "breadcrumbs.enabled": true,
    "window.menuBarVisibility": "toggle",
    "files.exclude": {
        "**/.pyc": true,
        "**/__pycache__": true
    },
    "search.exclude": {
        "**/.pyc": true,
        "**/__pycache__": true
    },
    "files.watcherExclude": {
        "**/.pyc": true,
        "**/__pycache__": true
    },
    "python.linting.flake8Args": [
        "--ignore=E116, E402, W503"
    ],
    "files.insertFinalNewline": true,
    "window.zoomLevel": 0,
    "workbench.colorCustomizations": {
        "breadcrumb.foreground": "#CDCDCD",
        "statusBar.background" : "#C62828",
        "statusBarItem.remoteBackground": "#C62828"
    },
    "editor.tokenColorCustomizations": {},
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,
    "workbench.sideBar.location": "left",
    "files.autoGuessEncoding": true,
    "editor.fontSize": 15,
    "explorer.autoReveal": true,
    "workbench.activityBar.visible": true,
    "vsnotes.defaultNotePath": "~/Documents/VSNotes",
    "editor.renderFinalNewline": true,
    "workbench.startupEditor": "newUntitledFile",
    "editor.renderWhitespace": "none",
    "editor.rulers": [
        79
    ],
    "python.autoComplete.addBrackets": true,
    "terminal.integrated.rendererType": "dom",
    "workbench.statusBar.visible": true,
    "fancyTerminal.foreground": "Black",
    "fancyTerminal.background": "Aqua",
    "editor.fontFamily": "Liberation Mono",
    "workbench.colorTheme": "Brogrammer",
    "python.linting.flake8Enabled": true,
    "editor.cursorStyle": "block",
    "editor.suggest.localityBonus": true
}
